# URI scheme for deep linking back to the app
SOYIO_URI_SCHEME=your_app_scheme

# Your Soyio company ID
SOYIO_COMPANY_ID=com_your_company_id

# Reference to identify the user in your system
SOYIO_USER_REFERENCE=user_123

# Disclosure template ID for identity verification flows
SOYIO_DISCLOSURE_TEMPLATE_ID=dtpl_your_template_id

# Authentication request ID
SOYIO_AUTH_REQUEST_ID=authreq_your_request_id

# Consent template ID for consent collection
SOYIO_CONSENT_TEMPLATE_ID=constpl_your_consent_template_id

# User email for disclosure flows
SOYIO_DISCLOSURE_USER_EMAIL=user@example.com

# Development URL for authentication flows (use localhost for iOS simulator, 10.0.2.2 for Android emulator)
SOYIO_AUTH_DEV_URL=http://localhost:3000

# Development URL for consent flows (use localhost for iOS simulator, 10.0.2.2 for Android emulator)
SOYIO_CONSENT_DEV_URL=http://localhost:3000

# Enable sandbox mode for testing (set to 'true' or 'false')
SOYIO_IS_SANDBOX=true
